llIntoView(el, { block: 'nearest' });\n      }\n    },\n  },\n  mounted() {\n    this.saveWidth();\n  },\n  updated() {\n    this.saveWidth();\n  },\n  methods: {\n    saveWidth() {\n      this.$store.commit('setNavWidth', this.nav.clientWidth);\n    },\n    goto(id: string) {\n      this.$store.commit('setHeading', id);\n    },\n  },\n});\n</script>\n\n\n<style lang=\"scss\" scoped>\n.nav {\n  position: fixed;\n  background-color: white;\n  max-width: 200px;\n  overflow: hidden;\n  top: 0;\n  left: 0;\n  height: 100%;\n  border-right: 1px solid rgba(0, 0, 0, 0.1);\n}\na {\n  text-decoration: none;\n  color: unset;\n}\n.heading {\n  margin: 10px 0;\n  padding: 0 1rem;\n  cursor: pointer;\n  white-space: nowrap;\n  border-right: 3px solid transparent;\n  user-select: none;\n  text-overflow: ellipsis;\n  overflow: hidden;\n  &.level-2 {\n    font-size: 16px;\n    color: #00162c;\n    font-weight: 500;\n    padding-top: 3px;\n    padding-bottom: 3px;\n  }\n  &.level-3 {\n    font-size: 12px;\n    color: #000000;\n    padding-top: 6px;\n    padding-bottom: 6px;\n    text-indent: 1rem;\n  }\n  &.level-4,\n  &.level-5,\n  &.level-6 {\n    display: none;\n  }\n  &.hash {\n    border-right: #30a5f9 3px solid;\n    background-color: #d5edfe;\n    color: #2ca7f8 !important;\n    font-weight: bold;\n  }\n}\n</style>\n\n\n"]}]}